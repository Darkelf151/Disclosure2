{
  "expo": {
    "name": "Disclosure Discounts",
    "slug": "disclosure-discounts",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/disclosurelogo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/white.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
