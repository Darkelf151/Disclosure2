    },
    "android": {
      "package": "com.foxwylie.disclosure_discounts",
      "versionCode": 24,
      "adaptiveIcon": {
        "foregroundImage": "./assets/white.png",
        "backgroundColor": "#000"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "89c7731c-ec1e-4e74-b3ef-10768a0f6c5b"
      }
    }
  }
